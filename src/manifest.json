{
  "manifest_version": 3,
  "name": "WebHarvest",
  "version": "1.2.0",
  "description": "Extract and analyze webpage source code with preserved structure",
  "permissions": [
    "activeTab",
    "scripting",
    "clipboardWrite",
    "downloads",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/logo-16.png",
    "32": "images/logo-32.png",
    "48": "images/logo-48.png",
    "128": "images/logo-128.png"
  }
}
